<%- template.use('feedbackModal'); %>

<script type="text/javascript" nonce="<%= helpers.cspNonce() %>">
$(document).ready(function() {
  $('input[placeholder]').placeholder();
  padmodals.initFeedback();
});
</script>


<div class="hp-pro-admin-fpcontent fpcontent">
  <%- template.use('button', null, {
    id: 'feedbackbutton',
    type: 'action',
    className: 'iphonehide',
    label: N_('feedback')
  }); %>

  <h1><%= helpers.siteName() %></h1>
  <br/>
  <div>
  <%- renderAdminLeftNav() %>
  </div>
  <div class="tab_container" style="background-color: white;">
    <div class="tab_content" style="padding: 40px;">
      <%- getAdminContent() %>
    </div>
  </div>
  &nbsp;
</div>



